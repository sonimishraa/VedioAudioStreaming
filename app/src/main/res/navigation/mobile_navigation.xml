<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.tamasha.vedioaudiostreamingapp.ui.dashboard.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_discover" />

    <fragment
        android:id="@+id/navigation_discover"
        android:name="com.tamasha.vedioaudiostreamingapp.ui.dashboard.discover.DiscoverFragment"
        android:label="@string/title_discover"
        tools:layout="@layout/fragment_discover" />

    <fragment
        android:id="@+id/navigation_play"
        android:name="com.tamasha.vedioaudiostreamingapp.ui.dashboard.play.PlayFragment"
        android:label="@string/title_play"
        tools:layout="@layout/fragment_club" />

    <fragment
        android:id="@+id/navigation_club"
        android:name="com.tamasha.vedioaudiostreamingapp.ui.dashboard.club.ClubFragment"
        android:label="@string/title_club"
        tools:layout="@layout/fragment_club" />

    <fragment
        android:id="@+id/navigation_refresh_earn"
        android:name="com.tamasha.vedioaudiostreamingapp.ui.dashboard.refreshearn.RefreshFragment"
        android:label="@string/refresh_earn"
        tools:layout="@layout/fragment_club" />
</navigation>